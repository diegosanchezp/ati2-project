{
  "compilerOptions": {
    "target": "es5",
    "baseUrl": ".",
    "paths": {
      "djtypes/*": ["../backend/tstypes/*"],
      "@/styles/*": ["styles/*"]
    },
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", "../backend/tstypes/*.ts"],
  "exclude": ["node_modules"]
}
